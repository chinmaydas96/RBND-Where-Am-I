<?xml version='1.0'?>

<robot name="my_robot" xmlns:xacro="http://www.ros.org/wiki/xacro">
  <xacro:include filename="$(find my_robot)/urdf/my_robot.gazebo"/>
<!--    <xacro:include filename="$(find my_robot)/urdf/materials.xacro" />-->
  <link name="robot_footprint"></link>

  <joint name="robot_footprint_joint" type="fixed">
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <parent link="robot_footprint"/>
    <child link="chassis"/>
  </joint>


  <link name='chassis'>

    <pose>0 0 0.1 0 0 0</pose>

    <inertial>
      <mass value="15.0"/>
      <origin xyz="0.0 0 0" rpy=" 0 0 0"/>
      <inertia ixx="0.1" ixy="0" ixz="0" iyy="0.1" iyz="0" izz="0.1"/>
    </inertial>

    <!-- <collision name='chassis_axle_collision'> <origin xyz="0.15 0 0" rpy="0 1.5707 1.5707"/> <geometry> <cylinder length="0.2" radius="0.025"/> </geometry> </collision> <visual name='chassis_axle_visual'> <origin xyz="0.15 0 0" rpy="0 1.5707
    1.5707"/> <geometry> <cylinder length="0.2" radius="0.025"/> </geometry> <material name="black"> <color rgba="0 0 0 1.0"/> </material> </visual>-->

    <!-- <collision name='chassis_main_collision'> <origin xyz="-0.05 0 -0.00" rpy=" 0 1.5707 0"/> <geometry> <cylinder length="0.3" radius="0.025"/> </geometry> </collision> <visual name='chassis_main_visual'> <origin xyz="-0.05 0 -0.00" rpy=" 0 1.5707
    0"/> <geometry> <cylinder length="0.3" radius="0.025"/> </geometry> <material name="dark_purple"> <color rgba="0.5 0.05 0.5 1.0"/> </material> </visual>-->
    <collision name='chassis_turret_collision'>
      <origin xyz="-0.0 0 -0.02" rpy=" 0 0 0"/>
      <geometry>
        <cylinder length="0.12" radius="0.1"/>
      </geometry>
    </collision>
    <visual name='chassis_turret_visual'>
      <origin xyz="-0.0 0 -0.02" rpy=" 0 0 0"/>
      <geometry>
        <cylinder length="0.12" radius="0.1"/>
      </geometry>
      <material name="green">
        <color rgba="0.3 0.33 0.12 1.0"/>
      </material>
    </visual>

    <visual name='chassis_main_base_visual'>
      <origin xyz="-0.0 0 -0.05" rpy=" 0 0 0"/>
      <geometry>
        <box size="0.2 0.2 0.06"/>
      </geometry>
    </visual>

    <collision name='chassis_front_collision'>
      <origin xyz="0.1 0 -0.05" rpy=" 0 0 0"/>
      <geometry>
        <cylinder length="0.06" radius="0.1"/>
      </geometry>
    </collision>
    <visual name='chassis_front_visual'>
      <origin xyz="0.1 0 -0.05" rpy=" 0 0 0"/>
      <geometry>
        <cylinder length="0.06" radius="0.1"/>
      </geometry>
    </visual>

    <collision name='chassis_rear_collision'>
      <origin xyz="-0.1 0 -0.05" rpy=" 0 0 0"/>
      <geometry>
        <cylinder length="0.06" radius="0.1"/>
      </geometry>
    </collision>
    <visual name='chassis_rear_visual'>
      <origin xyz="-0.1 0 -0.05" rpy=" 0 0 0"/>
      <geometry>
        <cylinder length="0.06" radius="0.1"/>
      </geometry>
    </visual>

    <collision name='lidar_mount_collision'>
      <origin xyz="-0.0 0 0.05" rpy=" 0 0 0"/>
      <geometry>
        <cylinder length="0.04" radius="0.04"/>
      </geometry>
    </collision>
    <visual name='lidar_mount_visual'>
      <origin xyz="-0.0 0 0.05" rpy=" 0 0 0"/>
      <geometry>
        <cylinder length="0.04" radius="0.04"/>
      </geometry>
      <material name="grey">
        <color rgba="0.7 0.7 0.7 1.0"/>
      </material>
    </visual>

    <!-- <collision name='chassis_caster_mount_collision'> <origin xyz="-0.15 0 -0.01" rpy=" 0 0 0"/> <geometry> <box size=".1 .1 .08"/> </geometry> </collision> <visual name='chassis_caster_mount_visual'> <origin xyz="-0.15 0 -0.01" rpy=" 0 0 0"/>
    <geometry> <box size=".1 .1 .08"/> </geometry> <material name="dark_purple"> <color rgba="0.5 0.05 0.5 1.0"/> </material> </visual>-->

    <collision name='chassis_camera_mount_collision'>
      <origin xyz="0.075 0 0.045" rpy=" 0 0 0"/>
      <geometry>
        <box size="0.1 0.05 0.05"/>
      </geometry>
    </collision>
    <visual name='chassis_camera_mount_visual'>
      <origin xyz="0.075 0 0.045" rpy=" 0 0 0"/>
      <geometry>
        <box size="0.1 0.05 0.05"/>
      </geometry>
<gazebo reference="chassis_camera_mount_visual">
      <material name="Gazebo/Orange"/>
</gazebo>>
    </visual>

    <collision name='front_caster_collision'>
      <origin xyz="0.15 0 -0.06" rpy=" 0 0 0"/>
      <geometry>
        <sphere radius="0.0399"/>
      </geometry>
    </collision>

    <visual name='front_caster_visual'>
      <origin xyz="0.15 0 -0.06" rpy=" 0 0 0"/>
      <geometry>
        <sphere radius="0.04"/>
      </geometry>
      <material name="dark_grey">
        <color rgba="0.1 0.01 0.01 1.0"/>
      </material>
    </visual>

    <collision name='back_caster_collision'>
      <origin xyz="-0.15 0 -0.06" rpy=" 0 0 0"/>
      <geometry>
        <sphere radius="0.0399"/>
      </geometry>
      <surface>
        <friction>
          <ode>
            <mu>0</mu>
            <mu2>0</mu2>
            <slip1>1.0</slip1>
            <slip2>1.0</slip2>
          </ode>
        </friction>
      </surface>
    </collision>

    <visual name='back_caster_visual'>
      <origin xyz="-0.15 0 -0.06" rpy=" 0 0 0"/>
      <geometry>
        <sphere radius="0.04"/>
      </geometry>
      <material name="dark_grey">
        <color rgba="0.1 0.01 0.01 1.0"/>
      </material>
    </visual>
  </link>

  <link name='left_wheel'>
    <inertial>
      <mass value="5"/>
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
      <!-- <inertia ixx="0.1" ixy="0" ixz="0" iyy="0.1" iyz="0" izz="0.1"/> -->
      <inertia ixx="0.08" ixy="0" ixz="0" iyy="0.08" iyz="0" izz="0.08"/>
    </inertial>

    <collision name='left_wheel_collision'>
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
      <geometry>
        <cylinder length="0.05" radius="0.08"/>
      </geometry>
    </collision>

    <visual name='left_wheel_visual'>
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
      <geometry>
        <cylinder length="0.05" radius="0.08"/>
      </geometry>
      <material name="dark_grey">
        <color rgba="0.05 0.05 0.05 1.0"/>
      </material>
    </visual>
  </link>

  <joint type="continuous" name="left_wheel_hinge">
    <origin xyz="0 0.15 -0.02" rpy="0 0 0"/>
    <child link="left_wheel"/>
    <parent link="chassis"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
    <limit effort="10000" velocity="1000"/>
    <dynamics damping="1.0" friction="1.0"/>
  </joint>

  <link name='right_wheel'>
    <inertial>
      <mass value="5"/>
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
      <inertia ixx="0.08" ixy="0" ixz="0" iyy="0.08" iyz="0" izz="0.08"/>
    </inertial>

    <collision name='right_wheel_collision'>
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
      <geometry>
        <cylinder length="0.05" radius="0.08"/>
      </geometry>
    </collision>

    <visual name='right_wheel_visual'>
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
      <geometry>
        <cylinder length="0.05" radius="0.08"/>
      </geometry>
      <material name="dark_grey">
        <color rgba="0.05 0.05 0.05 1.0"/>
      </material>
    </visual>
  </link>

  <joint type="continuous" name="right_wheel_hinge">
    <origin xyz="0 -0.15 -0.02" rpy="0 0 0"/>
    <child link="right_wheel"/>
    <parent link="chassis"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
    <limit effort="10000" velocity="1000"/>
    <dynamics damping="1.0" friction="1.0"/>
  </joint>

  <link name='camera'>
    <inertial>
      <mass value="0.1"/>
      <origin xyz="0.05 0.05 0.05" rpy="0 0 0"/>
      <inertia ixx="1e-6" ixy="0" ixz="0" iyy="1e-6" iyz="0" izz="1e-6"/>
    </inertial>

    <collision name='camera_collision'>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.05 0.05 0.05"/>
      </geometry>
    </collision>

    <visual name='camera_visual'>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.05 0.05 0.05"/>
      </geometry>
    </visual>
  </link>

  <joint type="fixed" name="camera_joint">
    <origin xyz="0.15 0 0.045" rpy="0 0 0"/>
    <child link="camera"/>
    <parent link="chassis"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
  </joint>

  <link name='hokuyo'>
    <inertial>
      <mass value="1e-5"/>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <inertia ixx="1e-6" ixy="0" ixz="0" iyy="1e-6" iyz="0" izz="1e-6"/>
    </inertial>

    <collision name='hokuyo_collision'>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.1 0.1 0.1"/>
      </geometry>
    </collision>

    <visual name='hokuyo_visual'>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://my_robot/meshes/hokuyo.dae"/>
      </geometry>
    </visual>
  </link>

  <joint type="fixed" name="hokuyo_joint">
    <origin xyz="0 0 0.1" rpy="0 0 0"/>
    <child link="hokuyo"/>
    <parent link="chassis"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
  </joint>


<gazebo reference="chassis">
  <material>Gazebo/Orange</material>
</gazebo>
</robot>
